{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "${mod_homepage}",
        "sources": "${mod_sources}",
        "issues": "${mod_issues}"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "org.orecruncher.dsurround.platform.fabric.FabricMod"
        ],
        "modmenu": [
            "org.orecruncher.dsurround.config.ModConfigMenu"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": ">=${minecraft_version}",
        "java": ">=17"
    },
    "suggests": {
        "cloth-config": ">=${clothapi_version}",
        "yet_another_config_lib_v3": ">=${yacl_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "dsurround.mod.link.credits": "https://github.com/OreCruncher/DynamicSurroundingsFabric/blob/main/CREDITS.md",
                "dsurround.mod.link.nashorn": "https://github.com/openjdk/nashorn"
            }
        },
        "dsurround": {
            "updateURL": "https://raw.githubusercontent.com/OreCruncher/DynamicSurroundingsFabric/main/versions.json",
            "curseForgeLink": "https://www.curseforge.com/minecraft/mc-mods/dynamic-surroundings-fabric-edition",
            "modrinthLink": "https://modrinth.com/mod/${modrinth_slug}"
        }
    }
}